<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap 
    PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" 
    "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="tractOtherManageXml">
   
   <select id="selectallothermanagepage" parameterClass="com.audit.entity.project.TractOtherManage" resultClass="com.audit.entity.project.TractOtherManage">
	     select * from pro_tractOtherManage where 1=1
	     and biaoDuanId=#biaoDuanId#
	    <dynamic prepend="order by">
             <isNotEmpty property="filed">
                       $filed$ $sort$
             </isNotEmpty>
         </dynamic>
	  
	  </select>
	  <!--
	   查询其它管理的记录条数 
	  -->
	  <select id="selectallothermanagecount" parameterClass="com.audit.entity.project.TractOtherManage" resultClass="java.lang.Integer">   
	    select count(1) from pro_tractOtherManage where 1=1
	     and biaoDuanId=#biaoDuanId#
	  </select>
	  
	  <!--  增加其它管理信息 -->
	 <insert id="addothermanage"  parameterClass="com.audit.entity.project.TractOtherManage">
	   insert into pro_tractOtherManage(id,biaoDuanId,datafile,dataname,createuser,createtime,remark) values
	    (#id#,#biaoDuanId#,#datafile#,#dataname#,#createuser#,#createtime#,#remark#)
	</insert>
	
	 <!-- 修改其它管理信息 -->
	<update id="updateothermanage" parameterClass="com.audit.entity.project.TractOtherManage">
	   update  pro_tractOtherManage
	   set
	  biaoDuanId=#biaoDuanId#,
	  datafile=#datafile#,
	  dataname=#dataname#,
	  createuser=#createuser#,
	  createtime=#createtime#,
	  remark=#remark#
	   where id=#id#
	</update>
	

	<!--  根据标段id查询其它管理信息  -->
	<select id="selecteothermanagebybdid" parameterClass="java.lang.String" resultClass="com.audit.entity.project.TractOtherManage">
	select * from pro_tractOtherManage  where biaoDuanId=#id#
	</select>
	
	<!-- 根据id查询其它管理对象 -->
	<select id="selecteothermanagebyid" parameterClass="java.lang.String" resultClass="com.audit.entity.project.TractOtherManage">
	select * from pro_tractOtherManage  where id=#id#
	</select>
	
	<!-- 根据id删除其它管理 -->
	<delete id="deleothermanage" parameterClass="java.lang.String">
	   delete from pro_tractOtherManage where id=#id#
	</delete>
    
</sqlMap>
